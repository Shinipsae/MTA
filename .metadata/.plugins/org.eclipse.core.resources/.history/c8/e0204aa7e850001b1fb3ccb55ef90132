import java.util.Scanner;

public class ex01 {

	public static void main(String[] args) {
		double num = 27;
		num %= -3d;
		num += 10f;
		num *= -4;
		System.out.println(num);
		
	}
	
	public static String getBirthdate() {
		System.out.println("»ıÀÏ yymmdd: ");
