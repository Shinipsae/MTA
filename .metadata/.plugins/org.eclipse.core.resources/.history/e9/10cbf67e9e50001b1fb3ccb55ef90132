package q38;

public class Q38 {

	public static void main(String[] args) {
		int n = 10;
		// valueOf()
		Integer iNum1 = Integer.valueOf(n);
		// casting
		Integer iNum2 = (Integer)n;
		// type 확인
		System.out.println(iNum1 instanceof Integer); // Integer가 맞냐
		System.out.println(iNum2 instanceof Integer); // Integer가 맞냐
	}

}
