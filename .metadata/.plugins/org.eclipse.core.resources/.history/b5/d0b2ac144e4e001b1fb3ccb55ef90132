package q07;

public class Q07 {

	public static void main(String[] args) {
		char data1 = 65;
		System.out.println(data1); // A
		
		long data2 = 65;
		System.out.println(data2); // 65
		
		float data3 = new Float("-65.0");
		System.out.println(data3); // -65.0
		
		short data4 = new Short("65.0");
		System.out.println(data4); // 예외발생

	}

}
